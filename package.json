{
  "name": "slate-react-starter",
  "version": "1.0.0",
  "private": true,
  "author": "Xian Yang Wong",
  "description": "A barebones, Slate compatible, starting point for developing Shopify themes with react.",
  "keywords": [
    "shopify",
    "theme",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xianyangwong/slate-react-typescript.git"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-typescript": "^7.3.3",
    "@shopify/slate-tools": "^1.0.0-beta.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-shopify": "^16.6.0",
    "browserlist": "^1.0.1",
    "concurrently": "^4.1.0",
    "core-js": "3",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "flexboxgrid-sass": "^8.0.5",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-prop-type-error": "^1.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "stylelint": "^10.0.0",
    "stylelint-declaration-strict-value": "^1.1.2",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.4.4",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0",
    "webpack": "4"
  },
  "scripts": {
    "start": "yarn compile && concurrently \"yarn watch:react\" \"slate-tools start --skipFirstDeploy\"",
    "start:slate": "slate-tools start",
    "watch": "slate-tools start --skipFirstDeploy",
    "watch:react": "ts-node --project tsconfig.compile.json tools/watchReactTemplates.ts",
    "build": "slate-tools build",
    "deploy": "slate-tools build && slate-tools deploy",
    "zip": "slate-tools build && slate-tools zip",
    "compile": "tsc"
  },
  "dependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.2.0",
    "husky": "^4.2.5",
    "jquery": "^3.4.1",
    "node-watch": "^0.6.4",
    "prop-types": "^15.7.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "typescript-fsa": "^3.0.0-beta-2"
  }
}
