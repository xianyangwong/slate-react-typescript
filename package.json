{
  "name": "slate-react-starter",
  "version": "1.0.0",
  "private": true,
  "author": "Xian Yang Wong",
  "description": "A barebones, Slate compatible, starting point for developing Shopify themes with react.",
  "keywords": [
    "shopify",
    "theme",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xianyangwong/slate-react-typescript.git"
  },
  "devDependencies": {
    "@shopify/slate-tools": "^1.0.0-beta.18",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-shopify": "^16.6.0",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0"
  },
  "scripts": {
    "start": "yarn compile && concurrently \"yarn watch:react\" \"slate-tools start\"",
    "start:slate": "slate-tools start",
    "watch": "slate-tools start --skipFirstDeploy",
    "watch:react": "ts-node --project tsconfig.compile.json tools/watchReactTemplates.ts",
    "build": "slate-tools build",
    "deploy": "slate-tools build && slate-tools deploy",
    "zip": "slate-tools build && slate-tools zip",
    "compile": "tsc"
  },
  "dependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "concurrently": "^5.2.0",
    "husky": "^4.2.5",
    "node-watch": "^0.6.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "typescript-fsa": "^3.0.0-beta-2"
  }
}
